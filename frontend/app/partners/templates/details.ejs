<div class="panel panel-primary">
  <div class="panel-heading">
    <%- t('partners', 'PANEL:TITLE:details') %>
  </div>
  <div class="panel-details">
    <div class="props first">
      <div class="prop">
        <div class="prop-name"><%- t('partners', 'PROPERTY:name') %></div>
        <div class="prop-value"><%= model.name || '-' %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('partners', 'PROPERTY:clientNo') %></div>
        <div class="prop-value"><%- model.clientNo || '-' %></div>
      </div>
      <div class="prop partners-address">
        <div class="prop-name"><%- t('partners', 'PROPERTY:address') %></div>
        <div class="prop-value"><%= model.address || '-' %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('partners', 'PROPERTY:supplierColor') %></div>
        <div class="prop-value">
          <% if (model.supplierColor) { %>
          <span class="label partners-color" style="background-color: <%= model.supplierColor %>"><%= model.supplierColor %></span>
          <% } else { %>
          -
          <% } %>
        </div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('partners', 'PROPERTY:receiverColor') %></div>
        <div class="prop-value">
          <% if (model.receiverColor) { %>
          <span class="label partners-color" style="background-color: <%= model.receiverColor %>"><%= model.receiverColor %></span>
          <% } else { %>
          -
          <% } %>
        </div>
      </div>
    </div>
  </div>
</div>
