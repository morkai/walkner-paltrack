<form class="well filter-form">
  <%- forms.dateTimeRange({
    idPrefix: idPrefix
  }) %>
  <% if (!user.data.partner) { %>
  <div class="form-group">
    <label for="<%- idPrefix %>-<%- firstPartnerProperty %>"><%- t('registry', 'PROPERTY:' + firstPartnerProperty) %></label>
    <input id="<%- idPrefix %>-<%- firstPartnerProperty %>" name="<%- firstPartnerProperty %>" type="text">
  </div>
  <% } %>
  <div class="form-group">
    <label for="<%- idPrefix %>-<%- secondPartnerProperty %>"><%- t('registry', 'PROPERTY:' + secondPartnerProperty) %></label>
    <input id="<%- idPrefix %>-<%- secondPartnerProperty %>" name="<%- secondPartnerProperty %>" type="text">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-docNo"><%- t('registry', 'PROPERTY:docNo') %></label>
    <input id="<%- idPrefix %>-docNo" class="form-control" name="docNo" type="text">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-limit"><%- t('registry', 'FILTER:limit') %></label>
    <input id="<%- idPrefix %>-limit" class="form-control" name="limit" type="number" value="25" min="5" max="1000" step="1">
  </div>
  <div class="form-group filter-actions">
    <button type="submit" class="btn btn-default"><i class="fa fa-filter"></i><span><%= t('registry', 'FILTER:submit') %></span></button>
  </div>
</form>
